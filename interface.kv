<GuzikWyglad>
    background_color: (0,1,1,0)
    #background_normal: ''
    canvas.before:
        Color:
            rgb: (0,1,1)
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [26]

<LED>
    canvas:
        Color:
            rgb: (69/255.0,69/255.0,69/255.0) if self.ledzik else (88/255.0, 255/255.0, 51/255.0)
        Ellipse:
            size: (10,10)
            pos: self.x, self.y

<Interface>:
    canvas:
        Color:
            rgb: (0.1, 0.1, 0.9)
        Rectangle:
            size: self.width, self.height
            #pos: self.x, self.y
    #BoxLayout:
    GuzikWyglad:
        text: "nie"
        id: naszguzik
        on_release: root.guzik_click()
        size_hint:(1, .3)
        LED:
            id:ledguzik1
            pos: self.x,self.height


    GuzikWyglad:
        id: naszguzik2
        on_release: root.guzik_click2()
        pos: 100,50
        LED:
            id:ledguzik2
            pos: self.x,self.height
    GuzikWyglad:
        id: naszguzik3
        on_release: root.guzik_click3()
        pos: 200,100
        LED:
            id:ledguzik3
            pos_hint: {'center_x': 0.2, 'center_y': 0.5}
    GridLayout:
        rows: 2
        cols: 3
        size: 100, 60
        spacing: 20
        padding: 10
        pos: root.width - 120, root.height - 70
        canvas.before:
            Color:
                rgb: (214/255.0, 222/255.0, 213/255.0)
            Rectangle:
                size: self.width, self.height
                pos: self.x, self.y
        LED:
            id: led1
        LED:
            id: led2
        LED:
            id: led3
        LED:
            id: led4
        LED:
            id: led5
        Button:
            id: naszguzikLED
            on_release: root.guzik_clickLED()


<DefButton>
    background_color: (0,1,1,0)
    #background_normal: ''
    canvas.before:
        Color:
            rgb: (0,1,1)
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [26]

<LED>
    canvas:
        Color:
            rgb: (69/255.0,69/255.0,69/255.0) if self.SelectedLED else (88/255.0, 255/255.0, 51/255.0)
        Ellipse:
            size: (10,10)
            pos: self.x, self.y

<Interface>:
    canvas:
        Color:
            rgb: (0.1, 0.1, 0.9)
        Rectangle:
            size: self.width, self.height
            pos: self.x, self.y
    DefButton:
        text: "nie"
        id: button1
        on_release: root.ButtonClick1()
        size_hint:(1, .3)
        LED:
            id:buttonLED1
            pos: button1.x+80,button1.y+80

    DefButton:
        id: button2
        on_release: root.ButtonClick2()
        pos: 100,50
        LED:
            id:buttonLED2
            pos: button2.x+ 80,button2.y + 80

    DefButton:
        id: button3
        on_release: root.ButtonClick3()
        pos: 200,100
        LED:
            id:buttonLED3
            pos: button3.x+80,button3.y+80



    GridLayout:
        rows: 2
        cols: 3
        size: 100, 60
        spacing: 20
        padding: 10
        pos: root.width - 120, root.height - 70
        canvas.before:
            Color:
                rgb: (214/255.0, 222/255.0, 213/255.0)
            Rectangle:
                size: self.width, self.height
                pos: self.x, self.y
        LED:
            id: led1
        LED:
            id: led2
        LED:
            id: led3
        LED:
            id: led4
        LED:
            id: led5
        Button:
            id: ButtonRNDM
            on_release: root.ButtonClickLED()


<GuzikWyglad>
    canvas:
        Color:
            rgb: (0,0,1) if self.selected else (0.2, 0.7, 0,9)
        Rectangle:
            size: self.width, self.height
            pos: self.x, self.y

<LED>
    canvas:
        Color:
            rgb: (69/255.0,69/255.0,69/255.0) if self.x else (88/255.0, 255/255.0, 51/255.0)
        Ellipse:
            pos:(100,200)
            size:(10,10)

<Interface>:
    canvas:
        Color:
            rgb: (1,0,1)
        Rectangle:
            size: self.width, self.height
            pos: self.x, self.y
    GuzikWyglad:
        id: naszguzik
        on_release: root.guzik_click()
    GuzikWyglad:
        id: naszguzik2
        on_release: root.guzik_click()
    GuzikWyglad:
        id: naszguzik3
        on_release: root.guzik_click()
    LED:
        id: led1